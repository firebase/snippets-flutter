rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /users/{document=**} {
      allow read, write;
    }

    match /cities/{document=**} {
      allow read, write;
    }

    match /restaurants/{document=**} {
      allow read, write;
    }
  }
}